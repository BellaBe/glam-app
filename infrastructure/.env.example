# --- Docker Registry ---
DOCKER_REGISTRY=glamyouup
IMAGE_TAG=latest

# --- Environment ---
APP_ENV=prod
LOG_LEVEL=WARNING

# --- Database ---
POSTGRES_USER=glam
POSTGRES_PASSWORD=changeme

# --- Security ---
JWT_SECRET=changeme-generate-with-openssl-rand-base64-32
TOKEN_ENCRYPTION_KEY=changeme-generate-with-openssl-rand-base64-32
INTERNAL_API_KEY=changeme-for-ai-services
SHOPIFY_WEBHOOK_SECRET=from-shopify-admin

# --- External Services ---
OPENAI_API_KEY=sk-...

# --- SMTP ---
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=noreply@glamyouup.com
SMTP_PASSWORD=app-password
