##########################
# Global Configuration
##########################
APP_ENV=dev

##########################
# Security
##########################
CLIENT_JWT_SECRET=your_client_jwt_secret_key
INTERNAL_JWT_SECRET=your_internal_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_SECONDS=180

##########################
# Shopify App
##########################
SHOPIFY_APP_URL=your_shopify_store_url
SHOPIFY_API_KEY=your_shopify_api_key
SHOPIFY_API_SECRET=your_shopify_api_secret
SHOPIFY_API_VERSION=2025-01
SHOPIFY_SCOPES=read_metaobject_definitions,read_metaobjects,read_orders,write_metaobject_definitions,write_metaobjects,write_orders,write_products,read_locations
SHOPIFY_SESSION_DB_ENABLED=true
SHOPIFY_SESSION_DB_USER=shopify_session_db_user
SHOPIFY_SESSION_DB_PASSWORD=your_shopify_session_db_password
SHOPIFY_SESSION_DB_NAME=shopify_session_db
SHOPIFY_SESSION_DB_PORT_EXTERNAL=5443

#########################
# Email Configuration
#########################
EMAIL_PROVIDER=mailhog
SMTP_HOST=mailhog
SMTP_PORT=1025
SMTP_USER=
SMTP_PASSWORD=

