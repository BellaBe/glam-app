# services/webhook-service/.env.example
# Database
DATABASE_URL=postgresql://webhook:password@localhost:5412/webhook_db

# Redis
REDIS_URL=redis://localhost:6312

# NATS
NATS_URL=nats://localhost:4212

# Shopify Webhook Secrets
SHOPIFY_API_SECRET=your-primary-webhook-secret
SHOPIFY_API_SECRET_NEXT=your-rotation-webhook-secret  # Optional

# Environment
ENVIRONMENT=development
LOG_LEVEL=DEBUG
